importPackage(org.wso2.carbon.pc.analytics.core.generic);

var log = new Log();
if(log.isDebugEnabled()){
 log.debug("User level monitoring getUserLevelAvgExecuteTimeVsTaskId logic");
}

var userLevelMonitor = new UserLevelMonitor();
var filters = String(request.getParameter("filters"));
if(log.isDebugEnabled()){
 log.debug("User Level Avg Execution Time Vs Task Id Filters:" + filters);
}
var userLevelAvgExecTimeVsTaskId = userLevelMonitor.getUserLevelAvgExecuteTimeVsTaskId(filters);
print(userLevelAvgExecTimeVsTaskId);
if(log.isDebugEnabled()){
 log.debug("User Level Avg Execution Time Vs Task Id Result:" + userLevelAvgExecTimeVsTaskId);
}
