importPackage(org.wso2.carbon.pc.analytics.core.generic);

var log = new Log();
if(log.isDebugEnabled()){
 log.debug("Process level monitoring getExecutionTimeVsProcessInstanceId logic");
}

var processLevelMonitor = new ProcessLevelMonitor();
var filters = String(request.getParameter("filters"));
if(log.isDebugEnabled()){
 log.debug("Execution Time Vs Process Instance Id Filters:" + filters);
}
var execTimeVsProcessInstanceId = processLevelMonitor.getExecutionTimeVsProcessInstanceId(filters);
print(execTimeVsProcessInstanceId);
if(log.isDebugEnabled()){
 log.debug("Execution Time Vs Process Instance Id Result:" + execTimeVsProcessInstanceId);
}
